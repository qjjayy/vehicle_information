<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.tongji_qjj.domain">
	<class name="Vehicle_Static" table="vehicle_static">
		<id name="vehicleID">
			<generator class="assigned"/>
		</id>
		
		<set name="dynamicDatum" cascade="all">
			<key column="vehicleID"/>
			<one-to-many class="Vehicle_Dynamic"/>
		</set>
		
		<property name="length"/>
		<property name="width"/>
		<property name="type"/>
		<property name="totalFrames"/>
	</class>
</hibernate-mapping>